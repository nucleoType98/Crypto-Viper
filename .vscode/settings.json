{
     "[cpp]": {
         "editor.defaultFormatter": "ms-vscode.cpptools"
     },
     "editor.fontWeight": "500",
     "files.autoSave": "afterDelay",
     "editor.guides.indentation": false,
     "editor.lineHeight": 1.2,
     "editor.letterSpacing": 1.7,
     "editor.defaultFormatter": "psycha0s.cpp-format",
     "editor.formatOnSave": true,
     "editor.formatOnType": true,
     "C_Cpp.clang_format_fallbackStyle": "Microsoft",
     "C_Cpp.clang_format_style": "{BasedOnStyle: LLVM, ColumnLimit: 580}",
     "terminal.integrated.fontFamily": "Noto Mono",
     "terminal.integrated.fontWeightBold": "800",
     "security.workspace.trust.untrustedFiles": "open",
     "zenMode.showTabs": "none",
     "editor.minimap.autohide": true,
     "editor.minimap.maxColumn":310,
     "editor.minimap.scale": 3,
     "editor.minimap.size": "proportional",
     "editor.fontSize": 25,
     "editor.fontFamily": "Terminus (TTF)",
     "scm.diffDecorations": "minimap",
     "editor.tabSize": 4,
     "githubPullRequests.remotes": [
         
         "git@github.com:nucleoType98/socket.git",
         "origin",
         "upstream",
     ],
     "git.autofetch": true,
     "git.repositoryScanIgnoredFolders": [],
     "github.gitProtocol": "ssh",
     "git.verboseCommit": true,
     "git.allowNoVerifyCommit": true,
     "remoteHub.gitProtocol": "ssh",
     "workbench.colorTheme": "GitHub Dark Mode",
     "C_Cpp.formatting": "clangFormat",
     "workbench.settings.applyToAllProfiles": [
         "C_Cpp.formatting",
         "C_Cpp.clang_format_fallbackStyle"
     ],
     "editor.tokenColorCustomizations": {
         "textMateRules": [
             {
                 "scope": [],
                 "settings": {
                     "fontStyle": ""
                 }
             }
         ]
    },
